{
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "generated-docs/jsdoc",
    "recurse": true,
    "template": "templates/default"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
