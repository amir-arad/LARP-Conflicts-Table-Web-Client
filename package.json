{
  "name": "larp-conflicts-table-web-client",
  "version": "1.0.0",
  "description": "LARP Conflicts Table Web Client",
  "type": "module",
  "platforms": {
    "node": "22"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md,json}\"",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run --reporter=dot",
    "docs:prepare": "mkdir generated-docs 2>nul || (exit 0) && mkdir generated-docs\\api generated-docs\\jsdoc generated-docs\\components generated-docs\\diagrams generated-docs\\coverage 2>nul || (exit 0)",
    "docs:typedoc": "typedoc",
    "docs:jsdoc": "jsdoc -c jsdoc.json || echo JSDoc generation skipped - TypeScript files not supported",
    "docs:compodoc": "compodoc -c .compodocrc.json",
    "docs:dependencies": "depcruise --include-only '^src' --output-type dot src | dot -T svg > generated-docs/diagrams/dependency-graph.svg || echo Dependency graph generation skipped - GraphViz not installed",
    "docs:module-graph": "depcruise --include-only '^src' --output-type archi src | dot -T svg > generated-docs/diagrams/module-graph.svg || echo Module graph generation skipped - GraphViz not installed",
    "docs:plantuml": "node scripts/generate-plantuml.js",
    "docs:coverage": "vitest run --coverage",
    "docs:integrate": "node scripts/integrate-docs.js",
    "docs:generate": "npm run docs:prepare && npm run docs:typedoc && npm run docs:jsdoc && npm run docs:compodoc && npm run docs:plantuml && npm run docs:coverage",
    "docs": "npm run docs:generate && npm run docs:integrate",
    "postinstall": "echo 'To install documentation dependencies, use: npm install --legacy-peer-deps'"
  },
  "dependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@google-cloud/local-auth": "^3.0.1",
    "@react-oauth/google": "^0.12.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "firebase": "^11.2.0",
    "gapi-script": "^1.2.0",
    "googleapis": "^144.0.0",
    "lucide-react": "^0.474.0",
    "postcss-loader": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "use-google-sheets": "^2.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@eslint/js": "^9.20.0",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-themes": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@tailwindcss/postcss": "^4.0.3",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/react": "^16.2.0",
    "@tsconfig/vite-react": "^3.4.0",
    "@types/gapi": "^0.0.47",
    "@types/gapi.auth2": "^0.0.61",
    "@types/gapi.client": "^1.0.8",
    "@types/gapi.client.sheets-v4": "^0.0.4",
    "@types/gapi.drive": "^0.0.9",
    "@types/lodash": "^4.17.15",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.20",
    "dependency-cruiser": "^16.10.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "genaiscript": "^1.111.3",
    "jsdoc": "^4.0.4",
    "jsdom": "^26.0.0",
    "lightningcss": "^1.29.1",
    "plantuml-encoder": "^1.4.0",
    "postcss": "^8.5.1",
    "postcss-dark-theme-class": "^1.3.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.5.3",
    "tailwindcss": "^4.0.3",
    "tsx": "^4.19.2",
    "type-fest": "^4.33.0",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  }
}
